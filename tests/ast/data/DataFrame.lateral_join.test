df1 = session.create_dataframe([[1, 2], [3, 4], [5, 6]], schema=["a", "b"])

df2 = session.create_dataframe([[1, 7], [3, 8]], schema=["a", "c"])

df1.lateral_join(df2).select('*')

df1.lateral_join(df2, df1.a == df2.a).select('*')

df1.lateral_join(df2, (df1.a == df2.a) & (df1.b * 2 > df2.c)).select('*')

df1.lateral_join(df2, how="left").select('*')

df1.lateral_join(df2, df1.a == df2.a, how="cross").select('*')

df1.lateral_join(df2, lsuffix="_L", rsuffix="_R").select('*')

df1.lateral_join(df2, how="cross", lsuffix="_L", rsuffix="_R").select('*')

df1.lateral_join(df2, df1.b < df2.a, "cross", lsuffix="_L", rsuffix="_R").select('*')


