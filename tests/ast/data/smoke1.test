filtered_users_df = users_df.filter((col("membership_type") == "Palladium") & (col("age") > 25))

expanded_visits_df = filtered_visits_df.with_column("visit_duration_minutes", datediff("minute", to_timestamp(col("start_time")), to_timestamp(col("end_time"))))

expanded_users_df = filtered_users_df.with_column("full_name", concat(col("first_name"), lit(" "), col("last_name")))

joined_df = expanded_visits_df.join(expanded_users_df, on="user_id", how="left_outer")

aggregated_df = joined_df.group_by("membership_type")

aggregated_df = aggregated_df.agg(avg("visit_duration_minutes").alias("avg_visit_duration"))

sorted_df = aggregated_df.sort(col("avg_visit_duration").desc())

sorted_df = sorted_df.limit(100, 0)

