## TEST CASE

# 1. Create dataframes from tables
visits_df = session.table(tables.visits)
users_df = session.table(tables.user_profiles)

# 2. Filter data based on conditions
# Filter website visits to only include those from 'USA'

filtered_visits_df = visits_df.filter(col("country") == 'USA')

users_df = users_df.with_column("age", datediff('year', current_date(), col("dob")))

# Filter user profiles to only include users who are 'Palladium' members and older than 25
filtered_users_df = users_df.filter((col("membership_type") == 'Palladium') & (col("age") > 25))

# 3. Expand the dataframe with computed columns
# For website visits, calculate visit duration in minutes
expanded_visits_df = filtered_visits_df.with_column(
    "visit_duration_minutes",
    datediff('minute', to_timestamp(col("start_time")), to_timestamp(col("end_time")))
)

# For user profiles, create a full name column
expanded_users_df = filtered_users_df.with_column(
    "full_name",
    concat(col("first_name"), lit(" "), col("last_name"))
)

# 4. Join dataframes
# Join visits data with user profile data on user_id
joined_df = expanded_visits_df.join(
    expanded_users_df,
    on="user_id",
    how="left"  # Left join to keep all visits, even if user profile is missing
)

# 5. Compute a simple aggregation
# Group by membership type and calculate average visit duration
aggregated_df = joined_df.group_by("membership_type").agg(
    avg("visit_duration_minutes").alias("avg_visit_duration")
)

# 6. Sort the outputs
# Sort by average visit duration in descending order
sorted_df = aggregated_df.sort(col("avg_visit_duration").desc()).limit(100, 0)

